{
   "cli": {
      "version": ">= 10.1.1"
   },
   "build": {
      "development": {
         "channel": "development",
         "developmentClient": true,
         "distribution": "internal",
         "ios": {
            "resourceClass": "medium"
         }
      },
      "preview": {
         "extends": "development",
         "channel": "preview",
         "ios": {
            "simulator": true
         }
      },
      "production": {
         "channel": "production",
         "android": {
            "image": "latest"
         },
         "ios": {
            "image": "latest"
         },
         "env": {
            "GOOGLE_SERVICES_FILE": "./GoogleService-Info.plist"
         }
      }
   },
   "submit": {
      "production": {}
   }
}
